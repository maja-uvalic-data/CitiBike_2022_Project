import pandas as pd
import numpy as np
from keplergl import KeplerGl



df = pd.read_csv("../Data/citibike_2022_with_weather_sample.csv", low_memory=False)
df.head()



df["trip_count"] = 1

df_grouped = (
    df.groupby(["start_station_name", "end_station_name"])["trip_count"]
      .sum()
      .reset_index()
      .rename(columns={"trip_count": "total_trips"})
)

print(type(df_grouped))
df_grouped.head()



print(type(df_grouped))


map_1 = KeplerGl(
    height=500,
    data={"Trips between stations": df_grouped}
)

print("Map object created.")



KeplerGl(data={"Trips between stations": df_grouped})



map_1.save_to_html(file_name="../Imgs/citibike_trips_map.html")
print("Map saved.")



